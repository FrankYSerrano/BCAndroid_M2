pipeline {
    agent any
    stages {
        stage('Clone del repositorio') {
            steps {
                git 'https://github.com/FrankYSerrano/Tareas.git/’ 
            }
        }
        stage('Construcción') {
            steps {
                sh 'javac -version'
                sh 'javac -sourcepath src -d build src/*.java'
            }
        }
        stage('Prueba') {
            steps {
                sh 'java -cp build MainTest'
            }
        }
    }
}
